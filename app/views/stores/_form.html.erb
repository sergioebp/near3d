<%= form_with(model: store, local: true) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <h5>Deje en blanco para generar coordenadas automaticamente</h5>

    <%= f.label :latitude %>
    <%= f.text_field :latitude %>

    <%= f.label :longitude %>
    <%= f.text_field :longitude %>

    <%= f.submit %>
<% end %>